{
  "name": "webdriverio_boilerplate",
  "description": "UI Automation",
  "main": "index.js",
  "engines": {
    "node": ">=4.5.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rmdir ./node_modules",
    "e2e-setup": "./node_modules/selenium-standalone/bin/selenium-standalone install --drivers.chrome.version=2.37 --drivers.chrome.baseURL=https://chromedriver.storage.googleapis.com",
    "selenium-server": "./node_modules/selenium-standalone/bin/selenium-standalone start",
    "repl": "./node_modules/.bin/wdio repl chrome"
  },
  "keywords": [
    "selenium",
    "web",
    "testing",
    "webdriverjs",
    "webdriverio",
    "automation",
    "jasmine"
  ],
  "dependencies": {
    "@wdio/jasmine-framework": "^5.8.1",
    "@wdio/selenium-standalone-service": "^5.4.14",
    "@wdio/spec-reporter": "^5.8.0",
    "@wdio/sync": "^5.8.1",
    "dotenv": "^8.0.0",
    "jasmine-console-reporter": "^3.1.0",
    "webdriverio": "^5.8.2"
  },
  "devDependencies": {
    "@wdio/browserstack-service": "^5.8.0",
    "@wdio/cli": "^5.8.2",
    "@wdio/local-runner": "^5.8.2"
  }
}
